:graphite: <%= graphite_host %>
:templatedir: <%= template_dir %>

#:username: admin
#:password: secret
:options:
  :title: GDash
  :prefix: ""
  :refresh_rate: 60
  :graph_columns: 2
  :graph_width: 550
  :graph_height: 250
  :whisper_dir: "<%= whisper_dir %>"
  :interval_filters:
    - :label: Last 5 Minutes
      :from: -5minute
      :to: now
    - :label: Last Hour
      :from: -1hour
      :to: now
    - :label: Last Day
      :from: -1day
    - :label: Last Week
      :from: -1week
    - :label: Last Month
      :from: -1month
    - :label: Last Year
      :from: -1year
  :intervals:
    - [ "-5minute", "5 minutes" ]
    - [ "-1hour", "1 hour" ]
    - [ "-2hour", "2 hour" ]
    - [ "-1day", "1 day" ]
    - [ "-1month", "1 month" ]
    - [ "-1year", "1 year" ]

